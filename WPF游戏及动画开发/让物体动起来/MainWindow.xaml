<Window x:Class="让物体动起来.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:让物体动起来"
        mc:Ignorable="d" Title="WPFGame">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Button Grid.Row="0" Grid.Column="0" Name="btnStoryboardWindow" Click="btnStoryboardWindow_Click" Content="StoryboardWindow"/>
        <Button Grid.Row="0" Grid.Column="1" Name="btnCompositionTargetWindow" Click="btnCompositionTargetWindow_Click" Content="CompositionTargetWindow"/>
        <Button Grid.Row="0" Grid.Column="2" Name="btnDispatcherTimerWindow" Click="btnDispatcherTimerWindow_Click" Content="DispatcherTimerWindow"/>
    </Grid>
</Window>
